package segundoBimestre;

public class Lista {
	protected static Nodo head;
	protected long size;

	public Lista() {
		head = null;
		size= 0; 
	}
	
	public void AddFirst(Nodo v)
	{
		v.setNext(head);
		head  = v;
		size  = size + 1;
	}
	
	public void RemoveFirts()
	{
		Nodo t;
		if (head == null)
			System.out.println("Error la lista esta vacía");
		t = head;
		head = head.getNext();
		head = head.getNext();
		t.setNext( null);
		size = size-1;
	}

	/*public void AddLast(Node v)
	{
		v.setNext(null);
		tail.setNext(v);
		tail=v;
		size =size+1;
	}*/
}
