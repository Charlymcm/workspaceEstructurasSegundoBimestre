		String preg_nodo = null, preg_nodo2 = null; 
		String preg_lista = null;
		
		Lista lista_1 = new Lista();
		
		preg_nodo = JOptionPane.showInputDialog("¿Desea ingresar un nodo nuevo?");
		if( preg_nodo == "N"){
			preg_nodo2 = JOptionPane.showInputDialog("¿Desea ingresar un nodo nuevo?");
			//implementar impresion de lista
		}
		else{
			preg_lista = JOptionPane.showInputDialog("Ingrese un elemento a la lista");
			Nodo preg_lista_nodo = new Nodo(preg_lista,null);
			lista_1.AddFirst(preg_lista_nodo);

		}




		return;

	}
}
